upstream olfy_app_server {
    server unix:/home/sushant/Desktop/django_project/run/gunicorn.sock fail_timeout=0;
}

server {
    listen   80;
    server_name example.com;
    client_max_body_size 4G;
    access_log /home/sushant/Desktop/django_project/logs/nginx-access.log;
    error_log /home/sushant/Desktop/django_project/logs/nginx-error.log;
location /static/ {
    alias   /home/sushant/Desktop/django_project/olfy/static/;
}

location / {
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header Host $http_host;
    proxy_redirect off;
    if (!-f $request_filename) {
    proxy_pass http://olfy_app_server;
    break;
    }
}
}